<Window x:Class="TestApp.SecondWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TestApp"
        mc:Ignorable="d"
        Title="SecondWindow" Height="720" Width="1280">
    <StackPanel Orientation="Vertical" Width="300">
        <Ellipse Fill="Aqua" Width="300" Height="200"/>
        <Rectangle Fill="Red" Width="250" Height="150"/>
        <Path Height="100" Width="300">
            <Path.Data>
                <!-- Rect="왼쪽Padding, 위쪽Padding, 사각형 가로길이, 사각형 세로길이" -->
                <RectangleGeometry Rect="100, 0, 100, 100"/>
            </Path.Data>
            <Path.Fill>
                Blue
            </Path.Fill>
        </Path>
    </StackPanel>
</Window>
